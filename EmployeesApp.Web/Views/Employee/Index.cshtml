@model EmployeesApp.Web.Models.Employee[]
<!DOCTYPE html>
    <html>
    <head>
        <title>Employees</title>
    </head>
    <body>
    <h2>Employees</h2>
    <ol>
        @foreach (var employee in Model)
        {
            <li>@employee.Name - @employee.Email</li>
        }
        </ol>
    </body>
    </html>
